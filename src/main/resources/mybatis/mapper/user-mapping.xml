<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="user.UserDAO">

	<insert id="insertUser" parameterType="userDTO">
		insert into user(id, password, name, phone, email, region) values(#{id}, #{password}, #{name}, #{phone}, #{email}, #{region})
	</insert>
	
	<select id="duplicateCheck" parameterType="HashMap" resultType="int">
		select count(*) from user where ${id} = #{value};
	</select>
	
</mapper>